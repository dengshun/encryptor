<?xml version="1.0" encoding="utf-8" ?>
<!--项目常用编译开关-->
<flex-config>
	<!--SWF版本号-->
	<target-player>10.3</target-player>

	<compiler>
		<!--是否使用严格语法模式编译-->
		<strict>true</strict>

		<!--项目引用到的外部代码-->
		<source-path append="true">
			<path-element>../src</path-element>
			<path-element>../libs</path-element>
			<path-element>../../dump/src</path-element>
		</source-path>
	</compiler>
	<file-specs>
		<path-element>../src/EyptWrapper.as</path-element>
	</file-specs>
	
	<output>../bin/wrapper.swf</output>
</flex-config>